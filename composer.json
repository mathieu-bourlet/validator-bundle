{
  "name": "assoconnect/validator-bundle",
  "type": "symfony-bundle",
  "description": "Custom Symfony validators for common scalar types.",
  "keywords": [
    "Symfony",
    "Validation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "AssoConnect",
      "homepage": "https://www.assoconnect.com"
    },
    {
      "name": "Sylvain Fabre"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-filter": "*",
    "egulias/email-validator": "^2.1",
    "giggsey/libphonenumber-for-php": "^8.9",
    "jeremykendall/php-domain-parser": "^5.6",
    "symfony/config": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/filesystem": "^5.0",
    "symfony/http-kernel": "^5.0",
    "symfony/property-access": "^5.0",
    "symfony/validator": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "require-dev": {
    "doctrine/annotations": "~1.0",
    "doctrine/cache": "~1.0",
    "ergebnis/composer-normalize": "^2.3",
    "maglnet/composer-require-checker": "^2.1",
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/framework-bundle": "^5.0"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "AssoConnect\\ValidatorBundle\\": "."
    }
  },
  "scripts": {
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests",
    "normalize": "normalize",
    "test": "phpunit"
  }
}
